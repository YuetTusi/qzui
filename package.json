{
	"name": "qzui",
	"version": "1.0.0",
	"main": "main.js",
	"author": "北京万盛华通科技有限公司",
	"license": "MIT",
	"description": "N次方手机多路取证塔-专业版",
	"scripts": {
		"build": "webpack --config webpack.dev.js",
		"build:prod": "webpack --config webpack.prod.js",
		"build:dll": "webpack --config webpack.dll.js",
		"dev": "webpack-dev-server --config webpack.dev.js",
		"start": "electron .",
		"debug": "cross-env NODE_ENV=development concurrently \"yarn run dev\" \"yarn run start\" \"electron --inspect-brk=5858 .\"",
		"app": "cross-env NODE_ENV=development concurrently \"yarn run dev\" \"yarn run start\"",
		"prepack": "yarn run build:prod",
		"pack": "electron-builder --dir",
		"predist": "yarn run build:prod",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "qzui_1.0.0",
		"productName": "N次方手机多路取证塔-专业版",
		"copyright": "Copyright © 2020 北京万盛华通科技有限公司",
		"directories": {
			"buildResources": "static"
		},
		"files": [
			"dist/**/*",
			"node_modules/**/*",
			"packages.json",
			"main.js",
			"api.js",
			"src/config/**/*",
			"src/renderer/**/*",
			"icon.png"
		],
		"extraResources": [
			{
				"from": "src/config/conf",
				"to": "config/conf"
			},
			{
				"from": "icon.png",
				"to": "config/logo.png"
			},
			{
				"from": "data/base.db",
				"to": "data/base.db"
			}
		],
		"win": {
			"target": [
				"nsis",
				"zip"
			],
			"icon": "icon.png",
			"publisherName": "yuet",
			"artifactName": "${productName}.${ext}",
			"requestedExecutionLevel": "requireAdministrator"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true,
			"installerIcon": "installer.ico",
			"perMachine": true
		}
	},
	"dependencies": {
		"archiver": "^5.0.2",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"ini": "^1.3.5",
		"js-yaml": "^3.13.1",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"nedb": "^1.8.0",
		"node-notifier": "^6.0.0",
		"sqlite3": "^5.0.0"
	},
	"devDependencies": {
		"@types/archiver": "^3.1.1",
		"@types/classnames": "^2.2.10",
		"@types/color": "^3.0.1",
		"@types/ftp": "^0.3.31",
		"@types/ini": "^1.3.30",
		"@types/jquery": "^3.5.1",
		"@types/js-yaml": "^3.12.3",
		"@types/lodash": "^4.14.144",
		"@types/nedb": "^1.8.9",
		"@types/node-notifier": "^5.4.0",
		"@types/nunjucks": "^3.1.2",
		"@types/react": "^16.8.23",
		"@types/react-dom": "^16.8.4",
		"@types/redux-logger": "^3.0.7",
		"@types/requestidlecallback": "^0.3.1",
		"@types/uuid": "^3.4.5",
		"@types/webpack-env": "^1.14.0",
		"@ztree/ztree_v3": "^3.5.44",
		"antd": "^3.26.14",
		"axios": "^0.19.2",
		"classnames": "^2.2.6",
		"clean-webpack-plugin": "^3.0.0",
		"color": "^3.1.2",
		"concurrently": "^4.1.1",
		"cpy": "^8.1.1",
		"cross-env": "^5.2.0",
		"css-loader": "^3.1.0",
		"dva": "^2.4.1",
		"dva-immer": "^0.4.19",
		"electron": "^10.1.3",
		"electron-builder": "^22.9.1",
		"file-loader": "^4.1.0",
		"ftp": "^0.3.10",
		"guid-typescript": "^1.0.9",
		"html-webpack-plugin": "^3.2.0",
		"jquery": "^3.5.1",
		"less": "^3.9.0",
		"less-loader": "^5.0.0",
		"mini-css-extract-plugin": "^0.8.0",
		"nunjucks": "^3.2.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"redux-logger": "^3.0.6",
		"source-map-loader": "^0.2.4",
		"stickpackage": "^3.1.2",
		"style-loader": "^0.23.1",
		"style-resources-loader": "^1.3.3",
		"terser-webpack-plugin": "^2.2.1",
		"ts-loader": "^6.2.1",
		"typescript": "^3.8.3",
		"uuid": "^3.3.3",
		"webpack": "^4.36.1",
		"webpack-cli": "^3.3.6",
		"webpack-dev-server": "^3.7.2",
		"winston": "^3.2.1",
		"yaml-loader": "^0.6.0"
	}
}
